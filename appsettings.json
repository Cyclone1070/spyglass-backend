{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"MongoDbSettings": {
		"ConnectionString": "",
		"DatabaseName": "SpyglassDb"
	},
	"SearchSettings": {
		"MaxParallelism": 90,
		"CacheDurationMinute": 5,
		"SearchTimeoutSecond": 12
	},

	"ScraperRules": {
		"MegathreadUrls": [
			"https://fmhy.net/reading",
			"https://fmhy.net/video",
			"https://fmhy.net/gaming",
			"https://fmhy.net/downloading",
			"https://fmhy.net/linux-macos",
			"https://fmhy.net/mobile"
		],
		"MegathreadSkipKeywords": [
			"wiki",
			"github",
			"FOSS",
			"guide",
			"CSE",
			"reddit",
			"t.me",
			"mozilla",
			"greasyfork",
			"discord",
			"telegram",
			"vinegar",
			"guide",
			"launcher",
			"cli",
			"tui",
			"manager",
			"wine",
			"frontend",
			"invite",
			"mirrors"
		],
		"SearchSkipKeywords": [
			"category",
			"categories",
			"tag",
			"tags",
			"genre",
			"genres"
		],
		"Categories": [
			{
				"Name": "Books",
				"Selector": "#ebooks, #public-domain, #pdf-search"
			},
			{
				"Name": "Movies",
				"Selector": "#streaming-sites, #free-w-ads, #api-frontends, #single-server"
			},
			{
				"Name": "Anime",
				"Selector": "#anime-streaming"
			},
			{ "Name": "Games Download", "Selector": "#download-games" },
			{ "Name": "Games Repack", "Selector": "#game-repacks" },
			{
				"Name": "Abandonware/ROM",
				"Selector": "#abandonware-retro, #rom-sites, #nintendo-roms, #sony-roms"
			},
			{ "Name": "Windows Software", "Selector": "#software-sites" },
			{ "Name": "Mac", "Selector": "#software-sites-1, #mac-gaming" },
			{ "Name": "Android", "Selector": "#modded-apks, #untouched-apks" },
			{ "Name": "iOS", "Selector": "#ios-ipas" }
		],
		"CardFindingQueries": {
			"InvalidQuery": "asdfghjklqwerty12345",
			"ValidQueries": {
				"Books": ["sherlock", "harry potter"],
				"Movies": ["harry potter", "batman"],
				"Abandonware/ROM": ["mario", "zelda"]
			}
		}
	}
}
